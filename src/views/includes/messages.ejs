<% if(errors.length) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-danger">
          <% errors.forEach(erro => { %>
            <%= erro %><br>
           <% }) %> 
        </div>
    </div>
</div>
<% } %>

<% if(sucess.length) { %>
    <div class="row">
        <div class="col">
            <div class="alert alert-success">
              <% sucess.forEach(suces => { %>
                <%= suces %><br>
               <% }) %> 
            </div>
        </div>
    </div>
    <% } %>
